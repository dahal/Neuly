<h1>Listing sells</h1>

<table>
  <thead>
    <tr>
      <th>Regular</th>
      <th>Premium</th>
      <th>Domain</th>
      <th>Price</th>
      <th>Description</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @sells.each do |sell| %>
      <tr>
        <td><%= sell.regular %></td>
        <td><%= sell.premium %></td>
        <td><%= sell.domain %></td>
        <td><%= sell.price %></td>
        <td><%= sell.description %></td>
        <td><%= link_to 'Show', sell %></td>
        <td><%= link_to 'Edit', edit_sell_path(sell) %></td>
        <td><%= link_to 'Destroy', sell, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sell', new_sell_path %>
